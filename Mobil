package Modul3;

/**
 *
 * @author LENOVO
 */
public class Mobil implements Kendaraan {

    @Override
    public void bergerak() {
        System.out.println("Mobil bergerak di jalan raya.");
    }

    @Override
    public void berhenti() {
        System.out.println("Mobil berhenti di lampu merah.");
    }
}
